{
  "expo": {
    "name": "prism-studio",
    "slug": "prism-studio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/prism-studio.png",
    "scheme": "prismstudio",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/prism-studio.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/prism-studio.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.prismstudio",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/prism-studio.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3532975f-8fdb-4ce6-beb3-ee1d7341bc08"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/3532975f-8fdb-4ce6-beb3-ee1d7341bc08"
    }
  }
}
